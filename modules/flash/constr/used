pad_SYSCLK100_{P,N}             in

# to AMC[4..7]
pad_MGT224_REFCLK_{P,N}         in

# AMC PCIe first fat pipe
pad_AMC_PCI_RX_{P,N}[7..4]      in
pad_AMC_PCI_TX_{P,N}[7..4]      out

# Status LEDs
pad_FP_LED2A_K                  out
pad_FP_LED2B_K                  out
pad_FMC1_LED[1..4]              out

# SPI interface to SPI flash U105 (512Mbit) for custom state.
pad_USER_SPI_CS_L               out
pad_USER_SPI_SCK                out
pad_USER_SPI_D[3..0]            inout
# SPI interface to FPGA configuration flash
pad_FPGA_CFG_FCS2_B             inout
pad_FPGA_CFG_D[7..4]            inout

# I2C bus on FPGA slave port receiving mailbox writes
pad_FPGA_SLAVE_SCL              in
pad_FPGA_SLAVE_SDA              inout
