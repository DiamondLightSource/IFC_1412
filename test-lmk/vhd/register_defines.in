# Register definitions for test
!TOP
    GIT_VERSION     R
        .SHA        28
        -           3
        .DIRTY

    *RW
        STATUS      R
            .LMK_STATUS     2

        CONFIG      W
            .LMK_SELECT
            .LMK_RESET
            .LMK_SYNC

    LMK04616        RW
        # Data to write, returns data returned as part of last SPI transaction
        # when read
        .DATA       8
        .ADDRESS    15
        .R_WN
        # Selects which LMK to address
        .SELECT

    CLOCK_FREQ      R   4
